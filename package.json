    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^15.3.0",
    "react-cookie": "^1.0.4",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1",
    "react-touch-loader": "^1.1.0",
    "whatwg-fetch": "^2.0.1",
    "babel-polyfill": "^6.20.0",
    "bluebird": "^3.4.7",
    "kcors": "^1.3.1",
    "koa": "next",
    "koa-bodyparser": "3",
    "koa-router": "next",
    "koa-send": "^3.3.0",
    "log4js": "^1.1.0",
    "nock": "^9.0.2",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "sequelize": "^3.29.0",
    "sqlite3": "^3.1.8",
    "wechat-enterprise-api": "^0.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
