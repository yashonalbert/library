{
  "name": "szlibrary",
  "version": "1.0.0",
  "description": "szlibrary",
  "main": "app.js",
  "scripts": {
    "test": "rm -rf ./test/library.sqlite ./test/logs ./test/config.json ; cross-env WorkPath=./test mocha --timeout 5000 --compilers js:babel-core/register test/*.test.js",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha -- test/*.test.js",
    "electron": "babel src -d lib ; electron .",
    "nexe": "babel src -d lib ; node nexe.js",
    "role": "babel-node changeRole.js",
    "clean": "rimraf dist",
    "build-frontend": "npm run clean && cross-env NODE_ENV=production webpack --webpack.config.babel.js --progress",
    "build-backend": "babel src -d lib",
    "analyzer": "npm run clean && cross-env NODE_ENV=production webpack --webpack.config.babel.js --json | webpack-bundle-size-analyzer",
    "pack": "npm run build-frontend && rm -rf ../szlibrary/lib/public && cp -r ./dist ../szlibrary/lib/public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://YashonAlbert@bitbucket.org/YashonAlbert/szlibrary.git"
  },
  "author": "YashonAlbert",
  "license": "ISC",
  "homepage": "https://bitbucket.org/YashonAlbert/szlibrary#readme",
